{
  "version": "1.0.0",
  "name": "CTO Max Setup",
  "description": "Comprehensive CTO-level orchestration with all MCP tools, agents, and abilities activated",
  "repo": "Spot",
  "swarm": "enterprise-ai-mobile-infra",
  "rules": [
    "CTO Eleven MCP prefix all",
    "auto-deploy",
    "auto-test",
    "auto-sec"
  ],
  "activated": {
    "mcp_tools": "all",
    "agents": "all",
    "abilities": "all",
    "defaults": true
  },
  "persistence": {
    "git": true,
    "cursor_config": true,
    "path": ".cursor/config"
  },
  "keybindings": {
    "Cmd+E": "swarm"
  },
  "verification": {
    "commands": [
      "nx test",
      "MCP[security]",
      "ollama status"
    ],
    "auto_run": true
  },
  "git": {
    "auto_commit": true,
    "commit_message": "feat(auto-cto-max)"
  },
  "reload_cursor": true
}
