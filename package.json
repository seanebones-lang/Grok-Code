{
  "name": "grokcode-replica",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ci": "vitest --ci --coverage",
    "test:ui": "vitest --ui",
    "test:jest": "jest",
    "test:jest:watch": "jest --watch",
    "test:jest:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:cypress": "cypress run",
    "test:cypress:open": "cypress open",
    "test:cypress:headless": "cypress run --headless",
    "test:all": "npm run test && npm run test:e2e",
    "test:all:ci": "npm run test:ci && npm run test:e2e",
    "analyze": "ANALYZE=true next build",
    "lighthouse": "npx lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json --chrome-flags=\"--headless --no-sandbox\"",
    "lighthouse:ci": "npx @lhci/cli@latest autorun --config=.lighthouserc.js",
    "postinstall": "prisma generate || echo 'Static deploy - Prisma skipped'",
    "prepare": "prisma generate || true",
    "railway:build": "prisma generate && prisma migrate deploy && next build",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "clean": "rm -rf .next node_modules/.cache"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@google/generative-ai": "^0.24.1",
    "@monaco-editor/react": "^4.6.0",
    "@octokit/rest": "^22.0.1",
    "@prisma/client": "^7.3.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/nextjs": "^7.91.0",
    "@tanstack/react-query": "^5.17.9",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.29.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "focus-trap-react": "^11.0.6",
    "framer-motion": "^12.24.12",
    "heroicons": "^2.1.5",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.562.0",
    "next": "15.0.7",
    "next-auth": "^5.0.0-beta.25",
    "next-themes": "^0.4.6",
    "octokit": "^5.0.5",
    "prisma": "^5.17.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-markdown": "^9.0.1",
    "react-resizable-panels": "^4.3.2",
    "react-window": "^2.2.5",
    "remark-gfm": "^4.0.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@lhci/cli": "^0.15.1",
    "@next/bundle-analyzer": "^15.0.7",
    "@playwright/test": "^1.48.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "autoprefixer": "^10.4.20",
    "c8": "^10.1.3",
    "cypress": "^13.7.3",
    "eslint": "^9.18.0",
    "eslint-config-next": "15.0.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^13.0.1",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite-tsconfig-paths": "^6.0.5",
    "vitest": "^4.0.18"
  },
  "optionalDependencies": {
    "openai": "^4.104.0"
  }
}
